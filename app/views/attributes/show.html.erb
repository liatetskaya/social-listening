<%- model_class = Attribute -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:fs_id) %>:</strong></dt>
  <dd><%= @attribute.fs_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fs_is_active) %>:</strong></dt>
  <dd><%= @attribute.fs_is_active %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tw_id) %>:</strong></dt>
  <dd><%= @attribute.tw_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tw_is_active) %>:</strong></dt>
  <dd><%= @attribute.tw_is_active %></dd>
  <dt><strong><%= model_class.human_attribute_name(:keyword) %>:</strong></dt>
  <dd><%= @attribute.keyword %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              attribute_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_attribute_path(@attribute), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              attribute_path(@attribute),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
